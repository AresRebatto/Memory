<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Memory.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Memory"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Single Player"
        ContentTemplate="{DataTemplate local:SinglePlayer}"
        Route="SinglePlayer"/>
    <ShellContent
        Title="Multi Player"
        ContentTemplate="{DataTemplate local:MuliPlayer}"
        Route="MultiPlayer"/>

</Shell>
